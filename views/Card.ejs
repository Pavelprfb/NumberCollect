<!DOCTYPE html>
<html>
<head>
  <title>Card Example</title>
  <link rel="stylesheet" href="/Css/Card.css">
</head>
<body>

<div class="card-container">
  <hr/>
  <div class="profile_like_comment_share">
    <div class="profile-pic-like">
      <img class="profile-pic-img" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTvSqBb2ZzY3n5K7RznUC3CEedzP5EgLNSduQ&s"/>
      <img class="profile-pic-img" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRQT08_1dF0iNLYfRnL2lbqnlXg5QKKofxDew&s"/>
      <img class="profile-pic-img" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQQMTTKWHbTyyzamL-cIfbdezh5UdohgAiA-g&s"/>
      &nbsp; 270K
    </div>
    <div class="comment_share">
      <div>38K Comments</div>
      &nbsp;
      <div>19K Share</div>
    </div>
    &nbsp;
  </div>
  <hr/>

  <div class="like_comment_share">
    <div>Like</div>
    <div>Comment</div>
    <div>Share</div>
  </div>

  <hr/>
  <br/>

  <% cardData.forEach(function(data) { %>
    <div class="card-item">
      <img src="<%= data.picture %>" alt="<%= data.name %>" class="profile-pic" />
      <div>
        <div class="comment-box">
          <strong><%= data.name %></strong>
          <p><%= data.comment %></p>
        </div>
        <div class="comment-actions">
          <span><%= data.time %></span>
          <span class="action-btn">Like</span>
          <span class="action-btn">Reply</span>
          <span><%= data.reactions %></span>
        </div>
      </div>
    </div>
  <% }); %>
</div>

</body>
</html>